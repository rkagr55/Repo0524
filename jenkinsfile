pipeline {
    agent any
 
    stages {
        stage('Build') {
            steps {
                     withMaven {
                        bat "mvn clean compile test"
                    }
            } 
        }
    }
post{
    always { 
        publishHTML(
                [
                    allowMissing: false, 
                    alwaysLinkToLastBuild: false, 
                    keepAll: false, 
                    reportDir: '', 
                    reportFiles: 'index.html', 
                    reportName: 'HTML Report', 
                    reportTitles: '', 
                    useWrapperFileDirectly: true
                ]
        )
    }
}

}
